<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.TeenCash" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/purple_200</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/black</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_200</item>
        <item name="colorOnSecondary">@color/black</item>
        <item name="android:windowBackground">@color/black030f28</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="SplashScreen" parent="Theme.AppCompat.DayNight.NoActionBar">
        <item name="android:windowBackground">@drawable/ic_splash_screen_night</item>
    </style>

    <style name="FragmentScreen">
        <item name="background">@color/black030f28</item>
    </style>

    <style name="ProductDialogStyle" parent="Theme.AppCompat.DayNight.Dialog.Alert">
    </style>

    <style name="SelectedDot">
        <item name="android:textColor">@color/grey_6c</item>
        <item name="android:textSize">@dimen/f_35</item>
    </style>

    <style name="Dots">
        <item name="android:textColor">@color/blue414A61</item>
        <item name="android:textSize">@dimen/f_35</item>
    </style>

    <style name="button_solid">
        <item name="android:background">@drawable/bg_oval_btn_solid_night</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:fontFamily">@font/poppins_semibold</item>
        <item name="android:textSize">@dimen/sp_18</item>
    </style>

    <style name="button_stroke">
        <item name="android:background">@drawable/bg_oval_btn_stroke_night</item>
        <item name="android:textColor">@color/blue7584A3</item>
        <item name="android:fontFamily">@font/poppins_semibold</item>
        <item name="android:textSize">@dimen/sp_18</item>
    </style>

    <style name="title_24sp_poppinsBold_black29">
        <item name="android:textColor">@color/white</item>
        <item name="android:fontFamily">@font/poppins_bold</item>
        <item name="android:textSize">@dimen/sp_24</item>
    </style>

    <style name="title_14sp_poppins_regular_grey58">
        <item name="android:textColor">@color/whiteF9</item>
        <item name="android:fontFamily">@font/poppins_regular</item>
        <item name="android:textSize">@dimen/sp_14</item>
    </style>
</resources>